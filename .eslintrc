{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "react/forbid-prop-types": [true, { "forbid": "any", "checkContextTypes": true, "checkChildContextTypes": true }]
  },
  "globals": {
    "log": true
  }
}
